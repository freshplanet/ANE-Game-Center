	<id>com.freshplanet.ane.AirGameCenter</id>
	<versionNumber>1.0.0</versionNumber>
	<platforms>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libAirGameCenter.a</nativeLibrary>
				<initializer>AirGameCenterInitializer</initializer>
				<finalizer>AirGameCenterFinalizer</finalizer>
